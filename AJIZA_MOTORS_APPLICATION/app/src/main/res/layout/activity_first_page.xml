<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/mainbackground"
    tools:context=".FirstPage">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="288dp"
        android:layout_height="229dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/intoadmin"
        app:srcCompat="@drawable/bgimage" />


    <Button
        android:id="@+id/intocustomer"
        android:layout_width="238dp"
        android:layout_height="75dp"
        android:layout_marginTop="56dp"
        android:text="GARRI CHAYIE"
        android:background="@drawable/buttonbackground1"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/intodealer"
        android:layout_width="237dp"
        android:layout_height="77dp"
        android:text="GARRI DENE HAI"
        android:background="@drawable/buttonbackground1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/intocustomer"
        app:layout_constraintStart_toStartOf="@+id/intocustomer"
        app:layout_constraintTop_toBottomOf="@+id/intocustomer"
        app:layout_constraintVertical_bias="0.356" />

    <ImageButton
        android:id="@+id/intoadmin"
        android:layout_width="72dp"
        android:layout_height="64dp"
        android:background="@drawable/admin_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/admin_button"
        tools:ignore="VectorDrawableCompat" />


</androidx.constraintlayout.widget.ConstraintLayout>